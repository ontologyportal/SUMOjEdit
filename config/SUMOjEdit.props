# action labels for actions supplied by actions.xml
SUMOjEditPlugin.show-stats.label=Show statistics for buffer
SUMOjEditPlugin.check-errors.label=Check for SUO-KIF errors
SUMOjEditPlugin.format-select.label=format axioms
SUMOjEditPlugin.goto-defn.label=go to definition
SUMOjEditPlugin.browse-term.label=browse term in Sigma
SUMOjEditPlugin.setFOF.label=select FOF language
SUMOjEditPlugin.setTFF.label=select TFF language
SUMOjEditPlugin.chooseVamp.label=select Vampire
SUMOjEditPlugin.chooseE.label=select EProver
SUMOjEditPlugin.query-exp.label=query on highlighted expression
SUMOjEditPlugin.to-tptp.label=SUO-KIF to TPTP
SUMOjEditPlugin.from-tptp.label=SUO-KIF from TPTP

