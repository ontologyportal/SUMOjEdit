<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>SUMOjEdit</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually.
 To prevent the customizer from showing, create nbproject/project.properties file and enter
auxiliary.show.customizer=false
property there. Adding
auxiliary.show.customizer.message=<message>
 will show your customized message when someone attempts to open the customizer.  -->
            <name>SUMOjEdit</name>
            <properties>
                <property-file>nbproject/private/private.properties</property-file>
                <property-file>nbproject/project.properties</property-file>
            </properties>
            <folders>
                <source-folder>
                    <label>SUMOjEdit</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Source Packages</label>
                    <type>java</type>
                    <location>src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Test Unit</label>
                    <type>java</type>
                    <location>test/unit/java</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Test Integration</label>
                    <type>java</type>
                    <location>test/integration/java</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>deploy</target>
                </action>
                <action name="rebuild">
                    <script>${ant.script}</script>
                    <target>clean</target>
                    <target>deploy</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="run">
                    <script>${ant.script}</script>
                    <target>run.jedit</target>
                </action>
                <action name="test">
                    <script>${ant.script}</script>
                    <target>test</target>
                </action>
                <action name="debug">
                    <script>${ant.script}</script>
                    <target>debug.jedit</target>
                </action>
                <action name="run.single">
                    <script>${ant.script}</script>
                    <target>run-selected-file-in-java</target>
                    <context>
                        <property>run.class</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.single">
                    <script>${ant.script}</script>
                    <target>debug-selected-file-in-java</target>
                    <context>
                        <property>debug.class</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="test.single">
                    <script>${ant.script}</script>
                    <target>run-selected-test</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/integration/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="test.single">
                    <script>${ant.script}</script>
                    <target>run-selected-test</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/unit/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="run.single.method">
                    <script>${ant.script}</script>
                    <target>run-single-test-method</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/integration/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="run.single.method">
                    <script>${ant.script}</script>
                    <target>run-single-test-method</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/unit/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.test.single">
                    <script>${ant.script}</script>
                    <target>debug-selected-test</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/integration/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.test.single">
                    <script>${ant.script}</script>
                    <target>debug-selected-test</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/unit/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.single.method">
                    <script>${ant.script}</script>
                    <target>debug-single-test-method</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/integration/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.single.method">
                    <script>${ant.script}</script>
                    <target>debug-single-test-method</target>
                    <context>
                        <property>test.class</property>
                        <folder>test/unit/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="run.single">
                    <script>${ant.script}</script>
                    <target>run-selected-file-in-java</target>
                    <context>
                        <property>run.class</property>
                        <folder>test/integration/java</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>${build.classes.dir}</location>
                <script>${ant.script}</script>
                <build-target>deploy</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>build/test/classes</location>
                <script>${ant.script}</script>
                <build-target>deploy</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>Source Packages</label>
                        <location>src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Test Unit</label>
                        <location>test/unit/java</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Test Integration</label>
                        <location>test/integration/java</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                    <ide-action name="debug"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/5">
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">${javac.classpath}</classpath>
                <built-to>${build.classes.dir}</built-to>
                <source-level>11</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>test/unit/java</package-root>
                <unit-tests/>
                <classpath mode="compile">${javac.test.classpath}</classpath>
                <built-to>${build.test.classes.dir}</built-to>
                <source-level>11</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>test/integration/java</package-root>
                <unit-tests/>
                <classpath mode="compile">${javac.test.classpath}</classpath>
                <built-to>${build.test.classes.dir}</built-to>
                <source-level>11</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
